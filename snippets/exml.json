"eui Button": {
		"prefix": "euiButtonSkin",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\" ?>",
			"<e:Skin class=\"$1\" states=\"up,down,disabled\" minHeight=\"50\" minWidth=\"100\" xmlns:e=\"http://ns.egret.com/eui\">",
			"\t<e:Image width=\"100%\" height=\"100%\" scale9Grid=\"1,3,8,8\" alpha.disabled=\"0.5\"",
			"\t\t\tsource=\"$2\"",
			"\t\t\tsource.down=\"$3\"/>",
			"\t<e:Label id=\"labelDisplay\" top=\"8\" bottom=\"8\" left=\"8\" right=\"8\"",
			"\t\t\tsize=\"20\" fontFamily=\"Tahoma 'Microsoft Yahei'\"",
			"\t\t\ttextColor=\"0xFFFFFF\" verticalAlign=\"middle\" textAlign=\"center\"/>",
			"\t<e:Image id=\"iconDisplay\" horizontalCenter=\"0\" verticalCenter=\"0\"/>",
			"</e:Skin>"
