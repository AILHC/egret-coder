			"\t<e:Image width=\"100%\" height=\"100%\" scale9Grid=\"1,3,8,8\" source=\"$2\"/>",
			"\t<e:Rect height=\"100%\" width=\"100%\" fillColor=\"0xffffff\"/>",
			"\t<e:EditableText id=\"textDisplay\" verticalCenter=\"0\" left=\"10\" right=\"10\"",
			"\t\t\ttextColor=\"0x000000\" textColor.disabled=\"0xff0000\" width=\"100%\" height=\"24\" size=\"20\" />",
			"\t<e:Label id=\"promptDisplay\" verticalCenter=\"0\" left=\"10\" right=\"10\"",
			"\t\t\ttextColor=\"0xa9a9a9\" width=\"100%\" height=\"24\" size=\"20\" touchEnabled=\"false\" includeIn=\"normalWithPrompt,disabledWithPrompt\"/>",
			"</e:Skin>"
		],
		"description": "eui TextInput Skin"
	},
"eui ToggleSwitchSkin": {
		"prefix": "euiToggleSwitchSkin",
		"body": [
			"<?xml version=\"1.0\" encoding=\"utf-8\"?>",
			"<e:Skin class=\"$1\" states=\"up,down,disabled,upAndSelected,downAndSelected,disabledAndSelected\" xmlns:e=\"http://ns.egret.com/eui\">",
			"\t<e:Image source=\"$2\"",
			"\t\t\tsource.up=\"$3\"",
			"\t\t\tsource.down=\"$4\"",
			"\t\t\tsource.disabled=\"$5\"/>",
			"\t<e:Image source=\"$6\"",
			"\t\t\thorizontalCenter=\"-18\"",
