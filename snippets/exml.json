			"\t<e:Group width=\"100%\" height=\"100%\">",
			"\t\t<e:layout>",
			"\t\t\t\t<e:HorizontalLayout verticalAlign=\"middle\"/>",
			"\t\t</e:layout>",
			"\t\t\t<e:Image fillMode=\"scale\" alpha=\"1\" alpha.disabled=\"0.5\" alpha.down=\"0.7\"",
			"\t\t\t\t\tsource=\"$2\"",
			"\t\t\t\t\tsource.upAndSelected=\"$3\"",
			"\t\t\t\t\tsource.downAndSelected=\"$4\"",
			"\t\t\t\t\tsource.disabledAndSelected=\"$5\"/>",
